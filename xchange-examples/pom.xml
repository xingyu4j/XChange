<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.knowm.xchange</groupId>
		<artifactId>xchange-parent</artifactId>
		<version>5.0.5-SNAPSHOT</version>
	</parent>

	<artifactId>xchange-examples</artifactId>

	<name>XChange Examples</name>
	<description>提供一组示例，这些示例演示如何在客户端应用程序中使用XChange</description>

	<url>http://knowm.org/open-source/xchange/</url>
	<inceptionYear>2012</inceptionYear>

	<organization>
		<name>Knowm Inc.</name>
		<url>http://knowm.org/open-source/xchange/</url>
	</organization>

	<!-- Parent provides default configuration for dependencies -->
	<dependencies>

		<dependency>
			<groupId>ch.qos.logback</groupId>
			<artifactId>logback-classic</artifactId>
		</dependency>
		<dependency>
			<groupId>org.knowm.xchart</groupId>
			<artifactId>xchart</artifactId>
		</dependency>

		<dependency>
			<groupId>org.reflections</groupId>
			<artifactId>reflections</artifactId>
		</dependency>

		<!-- XChange dependencies -->
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>xchange-core</artifactId>
			<version>${project.version}</version>
		</dependency>
		<!-- XChange Binance exchange support -->
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>xchange-binance</artifactId>
			<version>${project.version}</version>
		</dependency>
		<!-- XChange Bitstamp exchange support -->
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>xchange-bitstamp</artifactId>
			<version>${project.version}</version>
		</dependency>
		<!-- XChange Huobi exchange support -->
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>xchange-huobi</artifactId>
			<version>${project.version}</version>
		</dependency>
		<!-- XChange OkCoin exchange support -->
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>xchange-okcoin</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>xchange-deribit</artifactId>
			<version>${project.version}</version>
		</dependency>
	</dependencies>

</project>
